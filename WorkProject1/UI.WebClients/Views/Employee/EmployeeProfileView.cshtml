                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group" ng-class="{ 'has-error' : submitted && addBasic.MPOIndexNo.$invalid}">
                                                        <label class="col-md-5 control-label">
                                                            Index No:
                                                        </label>
                                                        <div class="col-md-7">
                                                            <input type="text" ng-model="employeeInfo.MPOIndexNo" class="form-control" placeholder="MPOIndexNo" name="MPOIndexNo">
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="col-md-4">
                                                    <label class="col-md-5 control-label">

                                                    </label>
