@section Scripts{

    <script type="text/javascript">

        OEMSApp.controller("MyProfile", ["$scope", "$http", "SweetAlert", "$window",
              function ($scope, $http, SweetAlert, $window) {

                  init();
                  function init() {
                      initialize();
                      getEmpInfoData();
                  }
                  function initialize() {
                      $scope.res = {};
                      $scope.data = {};
                      $scope.Employee = {};

                      $scope.Employee.EmpBasicInfo = [];
                      $scope.Employee.EmployeeEducationalInfo = [];
                      $scope.Employee.EmpJobHistory = [];
                      $scope.Employee.EmpTraining = [];
                      $scope.Employee.EmpNominee = [];

                      $scope.resetForm = resetForm;
                  }


                  function resetForm() {
                      $scope.submitted = false;
                  }

                  function getEmpInfoData() {
                      $scope.Dropdowns = [];
                      $http({
                          method: 'GET',
                          url: baseUrl + 'Employee/GetEmployeeInfo/'+'@ViewBag.EmpId'
                      }).then(function successCallback(response) {
                          $scope.Employee = response.data.results;
                          console.log(response.data.results);
                          console.log($scope.Employee.EmpBasicInfo);
                          //console.log($scope.Employee.EmployeeEducationalInfo)
                      }, function errorCallback(response) {
                          toastr["info"](response.data.message);
                      });
                  }

              }]);
    </script>
    <script>

        $('.browse-button').bind("click", function () {
            $('#UploadedFile').click();
        });

        function readURL(input) {
            if (input.files && input.files[0]) {
                var reader = new FileReader();

                reader.onload = function (e) {
                    $('.preview-img')
                        .attr('src', e.target.result);
                };

                reader.readAsDataURL(input.files[0]);
            }
        }
    </script>

}