                                                                <span ng-show="submitted && addBasic.MPOIndexNo.$error.required" class="help-block">Index No Required</span>
                                                                <div class="form-control-focus"> </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
