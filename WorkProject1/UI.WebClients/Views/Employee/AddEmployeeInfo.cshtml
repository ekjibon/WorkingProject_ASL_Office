                      $scope.ExamNames = [];
                      $scope.EmpExamBoard = {};
                      $scope.EmpExamBoards = [];
                      $scope.AddEducationalInfo = AddEducationalInfo;
                      $scope.AddEducationInObject = AddEducationInObject;
                      $scope.deleteEducation = deleteEducation;
                      $scope.ClickedExam = {};
                      $scope.EditEducation = EditEducation;


                      $scope.GetEmpExamGroup = GetEmpExamGroup;
                      $scope.EmpExamGroup = {};
                      $scope.EmpExamGroups = [];

