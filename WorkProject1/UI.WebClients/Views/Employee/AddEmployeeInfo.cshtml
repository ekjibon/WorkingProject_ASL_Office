                          $scope.educationInfo.GroupName = $("#eGName option:selected").text();
                          $scope.educationInfo.BoardName = $("#eBName option:selected").text();

                          $scope.educationInfo.IsDeleted = false;
                          $scope.educationInfo.EmployeeID = $scope.employeeInfo.EmpBasicInfoID;
