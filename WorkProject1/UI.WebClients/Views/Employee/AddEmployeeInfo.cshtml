                      $scope.getContacts2 = getContacts2;

                      $scope.currentPage = 1;
                      $scope.pageno = 1;
                      $scope.pagesize = 10;
                      $scope.total_count = 12;
                      $scope.search = '';

                      $scope.Districts = [];
                      $scope.res = {};
                      $scope.data = {};
                      $scope.resetForm = resetForm;
                        
                  }

                  function resetForm() {
                      $scope.employeeInfo = {};
                      $scope.employeeInfo.EmpID = '';
                      $scope.submitted = false;
                      $scope.Id = '';

                  }
                  

                  // for Employee ID Create
                  function GenerateID() {

                      if ($scope.employeeInfo.DesignationID != null && $scope.employeeInfo.JoiningDate != null)
                      {

                          $http({
                              method: 'POST',
                              url: baseUrl + 'Employee/GetEmployeeID/',
                              data: $scope.employeeInfo
                          }).then(function successCallback(response) {
                              $scope.employeeInfo.EmpID = response.data[0].EmpID;
                              $scope.employeeInfo.EmpBasicInfoID = response.data[0].EmpBasicInfoID
                          }, function errorCallback(response) {

                          });
                      }
                      else
                      {
                          alert("Employee Designation and Joining Date is Needed");
                      }
                      
                  }
                  
                  $scope.edit = function (n) {
                      $scope.employeeInfo = n;
                      $scope.Id = n.Id;
                  };

                  $scope.save = function save() {

                      if ($scope.employeeInfo.EmpID == null || $scope.employeeInfo.EmpID == 'undefined') {
                          toastr["warning"]("Please Generate Student ID!!");
                          return;
                      }
                    
                      var files = $("#imgs").get(0).files;
                      $http({
                          method: 'POST',
                          url: baseUrl + '/Employee/AddEmployee/',

                          headers: { 'Content-Type': undefined },
                          transformRequest: function (data) {
                              var formData = new FormData();
                              formData.append('studentBasicInfo', JSON.stringify($scope.employeeInfo));
                              if (files.length > 0)
                                  formData.append("Img", files[0]);
                              return formData;
                          },
                          data: { studentBasicInfo: $scope.employeeInfo }

                      }).then(function successCallback(response) {
                          //$scope.employeeInfo.EmpBasicInfoID = response.data.results[0];
                          $scope.activetab = 'EmpContact';
                          toastr["info"](response.data.message);
                      });


                  };



                  function UpdateAllOthers()
                  {
                      if ($scope.employeeInfo.EmpID == null || $scope.employeeInfo.EmpID == 'undefined') {
                          toastr["warning"]("Please Generate Employee ID!!");
                          return;
                      }
                           
                      $http({
                          method: 'PUT',
                          url: baseUrl + '/Employee/UpdateAllOthers/',

                          data: $scope.employeeInfo 

                      }).then(function successCallback(response) {

                          //$scope.employeeInfo.EmpBasicInfoID = response.data.returnvalue;
                         // $scope.StudentIID = response.data.returnvalue;
                          toastr["info"](response.data.message);
                      });


                  }



                  function getAllDropdowns() {
                      $scope.Dropdowns = [];

                      $http({
