          function Dateconvert(str) {
              var re = /\/Date\(([0-9]*)\)\//;
              var m = str.match(re);
              var datestring = new Date(parseInt(m[1]));
              var date = new Date(datestring),
                  mnth = ("0" + (date.getMonth() + 1)).slice(-2),
                  day = ("0" + date.getDate()).slice(-2);
              return new Date([date.getFullYear(), mnth, day].join("-"));
          }
