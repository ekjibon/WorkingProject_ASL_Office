                            <div class="dropdown-quantity"></div>
                            <i class="dropdown-icon fa fa-angle-down"></i>
                        </div>
                        <div class="checkbox-dropdown-inner">
                            <input type="search" class="checkbox-dropdown-search" />
                            <button type="button" class="btn btn-uncheck-all">All Sales</button>
                            <ul class="checkbox-dropdown-list"></ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="creatIssu">
                    <a href="" data-toggle="modal" data-target="#creatModal">Create</a>
                </div>
            </div>
