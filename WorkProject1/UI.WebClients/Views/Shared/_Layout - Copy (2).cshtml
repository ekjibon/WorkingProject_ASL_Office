    <script type="text/javascript">
    var path = window.location.pathname;
    $(".page-sidebar-menu li a").each(function (key, value) {
        var url = $(this).attr('href');
        if (url == path) {
            $(this).parent('li').addClass('active open');
            $(this).children('span').addClass('active');
            $(this).parent('li').parent('li').addClass('open');
            $(this).parent('li').parent('ul').removeClass('sub-menu');
            $(this).parent().parent().parent().addClass('open')
            $(this).closest('li').addClass('active');
            $(this).closest('.nav-item').children('div').addClass('nvv');
        }
        if (path == '/') {
            $(this).closest('.nav-item').children('a').addClass('nvv');
        }
        })
    </script>
