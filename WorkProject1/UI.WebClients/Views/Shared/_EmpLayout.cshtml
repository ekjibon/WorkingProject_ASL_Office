        var url = $(this).attr('href');

        if (url == path) {
            $(this).parent('li').addClass('kt-menu__item--open 555');
            $(this).parent('li').parent('ul').removeClass('kt-menu__subnav');
            $(this).closest(' li').addClass('kt-menu__item--open');
            $(this).closest('.nav-item').children('div').addClass('nvv');
        }
        if (path == '/') {
            $(this).closest('.nav-item').children('a').addClass('nvv');
        }
        })
    </script>
