@{
    ViewBag.Title = "Filtered SMS";
}
<style>
    .panel-title > a:before {
        float: right !important;
        font-family: FontAwesome;
        content: "\f056";
        padding-right: 5px;
    }

    .panel-title > a.collapsed:before {
        float: right !important;
        content: "\f055";
    }

    .panel-title > a:hover,
    .panel-title > a:active,
    .panel-title > a:focus {
        text-decoration: none;
    }

    .slider {
        position: relative;
        text-align: left;
        border: 1px solid gray;
        height: 0.6em;
    }

        .slider .handle {
            position: absolute;
            z-index: 2;
            width: 1.2em;
            height: 1.2em;
            background-color: gray;
        }

    .handle {
        top: -0.3em;
        margin-left: -0.6em;
    }

    .slider .range {
        position: absolute;
        z-index: 1;
        display: block;
        font-size: .7em;
        background-color: #e6975f;
        top: 0;
        height: 100%;
    }

    .switch {
        position: relative;
        display: inline-block;
        width: 60px;
        height: 34px;
    }

    .radio-group label {
        overflow: hidden;
    }

    .radio-group input {
        /* This is on purpose for accessibility. Using display: hidden is evil.
    This makes things keyboard friendly right out tha box! */
        height: 1px;
        width: 1px;
        position: absolute;
        top: -20px;
    }

    .radio-group .not-active {
        color: #3276b1;
        background-color: #fff;
    }

    .nopadding {
        padding: 0px !important;
    }
</style>

<div class="portlet box blue-hoki" ng-controller="studentSearchrdCrlt">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-user-plus"></i>Advance Search
        </div>
    </div>
    <div class="portlet-body form">
        <!-- BEGIN FORM-->
        <form class="form-horizontal">
            <div class="form-body">
                <div class="row">
                    <div class="panel-group " id="accordion" role="tablist" aria-multiselectable="true">
                        <div class="panel panel-default panel-info">
                            <!--Academic Informatin--><div class="panel-heading" role="tab" id="headingOne">
                                <h4 class="panel-title">
                                    <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                                        Academic Infomation
                                    </a>
                                </h4>
                            </div>
                            <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
                                <div class="panel-body">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Branch :
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.Branch1" ng-options="Branch.BranchId as Branch.BranchName for Branch in CommonFeilds.Branch"></select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                       
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Class :
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.Class1" ng-change="GetSection()" ng-options="Class.ClassId as Class.ClassName for Class in CommonFeilds.ClassInfo"></select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                      
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Status:
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.Statuses1" ng-options="StudentType.Value as StudentType.Text for StudentType in StudentTypeArr"></select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Session :
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.Session1" ng-options="Session.SessionId as Session.SessionName for Session in CommonFeilds.Session"></select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Section :
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.Section1" ng-disabled="Sections.length > 1" ng-options="Section.SectionId as Section.SectionName for Section in Sections"></select>
                                                    <div class="form-control-focus"></div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Stu. Type:
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.StuType1" ng-options="StuType.StudentTypeId  as StuType.StudentTypeName for StuType in CommonFeilds.StuType"></select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    Shift :
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.Shift1">
                                                        <optgroup ng-show="!Shift.length" ng-repeat="op in CommonFeilds.Branch" label="{{op.BranchName}}">
                                                            <option ng-repeat="Shift in CommonFeilds.Shift | filter:{BranchId:op.BranchId}:true" value="{{Shift.ShiftId}}">
                                                                {{Shift.ShiftName}}
                                                            </option>
                                                        </optgroup>
                                                    </select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="col-md-5 control-label">
                                                    House :
                                                </label>
                                                <div class="col-md-7">
                                                    <select multiple multi-select ng-model="studentSearch.House1" ng-options="House.HouseId as House.HouseName for House in CommonFeilds.House"></select>
                                                    <div class="form-control-focus"> </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="panel panel-default panel-info">
                                <div class="panel-heading" role="tab" id="headingTwo">
                                    <h4 class="panel-title">
                                        <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                            Student Information
                                        </a>
                                    </h4>
                                </div>
                                <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                                    <div class="panel-body">
                                        <div class="row">
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            DOB Range :
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" uib-datepicker-popup="{{Dateformat}}" placeholder="Start Date" uib-datepicker-popup="{{format}}" ng-model="studentSearch.startDate" is-open="popup1.opened" ng-click="popup1.opened=true" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" readonly="readonly" />
                                                            <span class="form-control-focus"> </span>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" uib-datepicker-popup="{{Dateformat}}" placeholder="End Date" uib-datepicker-popup="{{format}}" ng-model="studentSearch.endDate" min-date="studentSearch.startDate" is-open="popup2.opened" ng-click="popup2.opened=true" datepicker-options="dateOptionsDOB" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" readonly="readonly" />
                                                            <span class="form-control-focus"> </span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-5">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            Admission Date:
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" uib-datepicker-popup="{{Dateformat}}" placeholder="Start Date" uib-datepicker-popup="{{format}}" ng-model="studentSearch.AdstartDate" is-open="popup3.opened" ng-click="popup3.opened=true" datepicker-options="dateOptionsAd" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" readonly="readonly" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" uib-datepicker-popup="{{Dateformat}}" placeholder="Start Date" uib-datepicker-popup="{{format}}" ng-model="studentSearch.AdendDate" is-open="popup4.opened" min-date="studentSearch.AdstartDate" ng-click="popup4.opened=true" datepicker-options="dateOptionsAd" ng-required="true" close-text="Close" alt-input-formats="altInputFormats" readonly="readonly" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Gender :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select class="form-control" multiple multi-select ng-model="studentSearch.Gender1" name="Gender" ng-options="studentSearch.Text as studentSearch.Text for studentSearch in Dropdowns | filter:Type='Gender'"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Religion :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select class="form-control" multiple multi-select ng-model="studentSearch.Religion1" name="Gender" ng-options="studentSearch.Value as studentSearch.Text for studentSearch in Dropdowns | filter:Type='Religion'"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        BloodGroup :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select class="form-control" multiple multi-select ng-model="studentSearch.BloodGroup1" name="Gender" ng-options="studentSearch.Value as studentSearch.Text for studentSearch in Dropdowns | filter:Type='Blood Group'"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Quota :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select class="form-control" multiple multi-select ng-model="studentSearch.Quota1" name="Gender" ng-options="StudentReport.Value as StudentReport.Text for StudentReport in Dropdowns | filter:Type='Quota'"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            Age Range :
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" placeholder="Min" ng-model="studentSearch.minAge" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" ng-blur="compare(studentSearch.minAge,$event.target,'Age Range')" min="{{studentSearch.minAge}}" ng-disabled="studentSearch.minAge == null" placeholder="Max" ng-model="studentSearch.maxAge" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            Height(FT):
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="number" min="0" class="form-control" placeholder="Min" ng-model="studentSearch.startHeight" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" ng-blur="compare(studentSearch.startHeight,$event.target,'Height')" placeholder="Max" ng-disabled="studentSearch.startHeight == null" ng-model="studentSearch.endHeight" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            Weight(KG):
                                                        </label>

                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" placeholder="Min" ng-model="studentSearch.startWeight" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" ng-blur="compare(studentSearch.startWeight,$event.target,'Weight')" placeholder="Max" ng-disabled="studentSearch.startWeight == null" ng-model="studentSearch.endWeight" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            PSC GPA :
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" max="5" limitToMax placeholder="Min" ng-model="studentSearch.minPSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" max="5" preventTypingGreater ng-blur="compare(studentSearch.minPSCGPA,$event.target,'PSC GPA')" min="0" placeholder="Max" ng-disabled="studentSearch.minPSCGPA == null" ng-model="studentSearch.maxPSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            JSC GPA :
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" placeholder="Min" ng-model="studentSearch.minJSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" ng-blur="compare(studentSearch.minJSCGPA,$event.target,'JSC GPA')" min="0" placeholder="Max" ng-disabled="studentSearch.minJSCGPA == null" ng-model="studentSearch.maxJSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            SSC GPA :
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="number" min="0" class="form-control" placeholder="Min" ng-model="studentSearch.minSSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" ng-blur="compare(studentSearch.minSSCGPA,$event.target,'SSC GPA')" min="0" ng-disabled="studentSearch.minSSCGPA == null" placeholder="Max" ng-model="studentSearch.maxSSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            HSC GPA :
                                                        </label>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" placeholder="Min" ng-model="studentSearch.minHSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" ng-blur="compare(studentSearch.minHSCGPA,$event.target,'SSC GPA')" min="0" placeholder="Max" ng-disabled="studentSearch.minHSCGPA == null" ng-model="studentSearch.maxHSCGPA" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Siblings :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <div class="btn-group btn-group-sm">
                                                            <label class="btn btn-xs red" ng-model="studentSearch.Sibling" uib-btn-radio="'True'">Yes</label>
                                                            <label class="btn btn-xs red" ng-model="studentSearch.Sibling" uib-btn-radio="'False'">No</label>
                                                            <label class="btn btn-xs red" ng-click="studentSearch.Sibling=null"><span class="fa fa-times-circle"></span></label>
                                                        </div>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Image :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <div class="btn-group">
                                                            <label class="btn btn-danger btn-xs" ng-model="studentSearch.Image" uib-btn-radio="'True'">Yes</label>
                                                            <label class="btn btn-danger btn-xs" ng-model="studentSearch.Image" uib-btn-radio="'False'">No</label>
                                                            <label class="btn btn-xs red" ng-click="studentSearch.Image=null"><span class="fa fa-times-circle"></span></label>
                                                        </div>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        SMS NO :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <div class="btn-group">
                                                            <label class="btn btn-danger btn-xs" ng-model="studentSearch.SMSno" uib-btn-radio="'True'">Yes</label>
                                                            <label class="btn btn-danger btn-xs" ng-model="studentSearch.SMSno" uib-btn-radio="'False'">No</label>
                                                            <label class="btn btn-xs red" ng-click="studentSearch.SMSno=null"><span class="fa fa-times-circle"></span></label>
                                                        </div>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Transport :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <div class="btn-group">
                                                            <label class="btn btn-danger btn-xs" ng-model="studentSearch.TransportFacility" uib-btn-radio="'True'">Yes</label>
                                                            <label class="btn btn-danger btn-xs" ng-model="studentSearch.TransportFacility" uib-btn-radio="'False'">No</label>
                                                            <label class="btn btn-xs red" ng-click="studentSearch.TransportFacility=null"><span class="fa fa-times-circle"></span></label>
                                                        </div>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Route :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select multiple multi-select class="form-control" ng-model="studentSearch.Routes1" name="Route" ng-options="Route.RouteId as Route.RouteName for Route in Routes"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Father's EQ :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select multiple multi-select class="form-control" ng-model="studentSearch.FatherEQ1" ng-options="studentSearch.Value as studentSearch.Text for studentSearch in Dropdowns | filter:Type='Qualification'"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Father's Profession :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <select multiple multi-select class="form-control" ng-model="studentSearch.FatherProfession1" ng-options="studentSearch.Value as studentSearch.Text for studentSearch in Dropdowns  | filter:Type='FatherProfession'"></select>
                                                        <div class="form-control-focus"> </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <div class="row">
                                                        <label class="col-md-4 control-label">
                                                            Father's Income :
                                                        </label>

                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control" min="0" placeholder="Min" ng-model="studentSearch.minFatherIncome" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>
                                                        <div class="col-md-4">
                                                            <input type="number" class="form-control " min="0" ng-blur="compare(studentSearch.maxFatherIncome,$event.target,'Father Income')" placeholder="Max" ng-model="studentSearch.maxFatherIncome" />
                                                            <div class="form-control-focus"> </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label class="col-md-5 control-label">
                                                        Father's Image :
                                                    </label>
                                                    <div class="col-md-7">
                                                        <div class="btn-group">
