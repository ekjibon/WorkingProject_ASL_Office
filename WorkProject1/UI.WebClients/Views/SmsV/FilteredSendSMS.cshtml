@section Scripts{
    <script type="text/javascript">
        OEMSApp.controller("studentSearchrdCrlt", ["$scope", "$http", "SweetAlert", "filterFilter",
          function ($scope, $http, SweetAlert, filterFilter) {
              init();
              function init() {
                  initialize();
                  getAllDropdowns();
                  getAllCommons();
                  getAllRoutes();
                  $scope.format = 'dd/MM/yyyy';
                  getAllSettings();
                  getStaticSMSTemplate();
                  getSMSBalance();
              }
              function initialize() {
                  $scope.studentSearch = {};
                  $scope.studentSearch.VersionID = 0;
                  $scope.studentSearch.SessionId = 0;
                  $scope.studentSearch.BranchID = 0;
                  $scope.studentSearch.ShiftID = 0;
                  $scope.studentSearch.ClassId = 0;
                  $scope.studentSearch.GroupId = 0;
                  $scope.studentSearch.SectionId = 0;
                  $scope.studentSearch.HouseId = 0;
                  $scope.studentSearch.StudentType = 0;
                  $scope.studentSearch.Search = "";
                  $scope.studentSearch = {};
                  $scope.sms = {};
                  $scope.Settings = {};
                  $scope.DestinationNo = 1;
                  $scope.sms.SMSPart = 0;
                  $scope.sms.SMSLen = 0;
                  $scope.Recipiant = 0;
                  $scope.SMSCounts = 0;
                  $scope.Settings = [];
                  $scope.sms.SMSBody = [];
                  $scope.studentSearchs = [];
                  $scope.Dropdowns = [];
                  $scope.DropdownGender = [];
                  $scope.DropdownStuType = [];
                  $scope.CommonFeilds = [];
                  $scope.BulkStudentInfo = [];
                  $scope.data = [];
                  $scope.DataV = [];
                  $scope.Districts = [];
                  $scope.Groups = new Array();
                  $scope.currentPage = 1;
                  $scope.pageno = 1;
                  $scope.pagesize = 10;
                  $scope.total_count = 12;

                  $scope.txtsearch = '';
                  $scope.getAllCommons = getAllCommons;
                  $scope.getAllDropdowns = getAllDropdowns;
                  $scope.getDistricts = getDistricts;
                  $scope.PStations1 = [];
                  $scope.PStations2 = [];
                  $scope.Routes = [];
                  $scope.Search = Search;
                  getDistricts();
                  datatimes();
                  $scope.getAllSettings = getAllSettings;
                  $scope.getStaticSMSTemplate = getStaticSMSTemplate;
                  $scope.nameEmpty = nameEmpty;
                  $scope.getSMSBalance = getSMSBalance;
                  $scope.SendBulkSMS = SendBulkSMS;
                  $scope.total = total;
                  $scope.pageSizeArray = [];
                  $scope.pageSizeArray.push({ value: 5, text: 5 });
                  $scope.pageSizeArray.push({ value: 10, text: 10 });
                  $scope.pageSizeArray.push({ value: 20, text: 20 });
                  $scope.pageSizeArray.push({ value: 50, text: 50 });
                  $scope.currentPage = 1;
                  $scope.pageSize = 10;
                  $scope.pageno = 1;
              };
              $scope.Reset = function Reset() {
                  $scope.studentSearch = [];
                  initialize();
                  getAllDropdowns();
                  getAllCommons();
              };
              function Recipiant() {
                  $scope.Recipiant = 0;
                  if ($scope.studentSearchs.length > 0) {
                      angular.forEach($scope.studentSearchs, function (item) {
                          if (item.Selected) {
                              $scope.Recipiant = $scope.Recipiant + 1;
                          }
                      });
                  }

                  $scope.SMSCounts = $scope.sms.SMSPart * $scope.Recipiant;
              }
              function total() {
                  if ($scope.sms.isUnicode) {

                      if ($scope.sms.SMSBody.length > 70) {
                          $scope.sms.SMSPart = Math.ceil($scope.sms.SMSBody.length / 67);
                          $scope.maximumLength = 67;
                      }
                      else {
                          $scope.sms.SMSPart = 1;
                          $scope.maximumLength = 70;
                      }
                  }
                  else {
                      if ($scope.sms.SMSBody.length > 160) {
                          $scope.sms.SMSPart = Math.ceil($scope.sms.SMSBody.length / 153);
                          $scope.maximumLength = 153;
                      }
                      else {
                          $scope.sms.SMSPart = 1;
                          $scope.maximumLength = 160;
                      }
                  }
                  Recipiant();
                  return $scope.sms.SMSPart;
              }
              $scope.containsNonLatinCodepoints = function containsNonLatinCodepoints(s) {
                  $scope.sms.isUnicode = /[^\u0000-\u00ff]/.test(s);
              }
              function nameEmpty() {
                  $scope.sms.TemplateId = 0;
              };
              function getAllSettings() {
                  $scope.Settings = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'SMS/GetAllSettings/'
                  }).then(function successCallback(response) {
                      $scope.Settings = response.data.results;
                      $scope.sms.isUnicode = $scope.Settings[0].AllowUnicode;
                      if ($scope.Settings[0].ForStudent == 1) {
                          $scope.IsStudentButton = true;
                      }
                      if ($scope.Settings[0].ForEmployee == 1) {
                          $scope.IsEmployeeButton = true;
                      }
                  }, function errorCallback(response) {
                      toastr["info"](response.data.message);
                  });
              }
              function getSMSBalance() {
                  $scope.SMSBalance = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'SMS/GetSMSBalance/'
                  }).then(function successCallback(response) {
                      $scope.SMSBalance = response.data.results;
                      var balance = $scope.SMSBalance.Result.TotalBalance;
                  }, function errorCallback(response) {
                      toastr["info"](response.data.message);
                  });
              }
              $scope.getSMSBody = function getSMSBody(id) {
                  $scope.Template = {};
                  $scope.Template = $scope.SMSTemplate.find(x => x.TemplateId === id);
                  $scope.sms.SMSBody = $scope.Template.SMSBody;

              };
              function getStaticSMSTemplate() {
                  $scope.SMSTemplate = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'SMS/GetStaticTemplate/'
                  }).then(function successCallback(response) {
                      $scope.SMSTemplate = response.data.results;
                  }, function errorCallback(response) {
                      toastr["warning"](response.Data.message);
                  });
              };
              $scope.getSMS = function getSMS() {
                  angular.forEach($scope.studentSearchs, function (value, key) {
                      if (value.Selected == true) {
                          $scope.BulkStudentInfo.push(value);
                      }
                  });
                  SendBulkSMS();
              };
              function SendBulkSMS() {
                  if ($scope.BulkStudentInfo.length > 0) {
                      $http({
                          method: 'POST',
                          data: $scope.BulkStudentInfo,
                          url: baseUrl + 'SMS/SendBulkSMS/' + $scope.sms.TemplateId + "/" + $scope.sms.SMSBody.length + "/" + $scope.sms.SMSPart + "/" + $scope.sms.SMSBody
                      }).then(function successCallback(response) {
                          $scope.submitted = false;
                          $scope.BulkStudentInfo = [];
                          toastr["info"](response.data.message);
                      }, function errorCallback(response) {
                          toastr["info"](response.data.message);
                      });
                  }
                  else {
                      toastr["info"]("Empty List");
                      return;
                  }
              };
              $scope.selectAll = function () {
                  angular.forEach($scope.studentSearchs, function (item) {
                      item.Selected = $scope.selectedAll;
                  });
                  Recipiant();
              };
              $scope.checkIfAllSelected = function () {
                  $scope.selectedAll = $scope.studentSearchs.every(function (item) {
                      return item.Selected == true
                  });
                  Recipiant();
              };
              function getAllDropdowns() {
                  $scope.Dropdowns = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Common/GetAllDropDown/'
                  }).then(function successCallback(response) {
                      $scope.Dropdowns = response.data.results;
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.message);
                  });
              }
              function getAllCommons() {
                  $scope.CommonFeilds = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Common/GetCommonItem/'
                  }).then(function successCallback(response) {
                      $scope.CommonFeilds = response.data.results;
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.message);
                  });
              }
              function getAllRoutes() {
                  $scope.Routes = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Student/GetAllRouteSetup/'
                  }).then(function successCallback(response) {
                      $scope.Routes = response.data.results;
                  }, function errorCallback(response) {
                  });
              }
              $scope.compare = function compare(a, b, c) {
                  if (a > b.value) {
                      toastr["warning"]("Please Check Min and Max " + c);
                      $(b).focus();
                      c = null;
                  }
              }
              $scope.GetClass = function GetClass() {
                  debugger;
                  if ($scope.studentSearch.Version.length > 1)
                      return;
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Common/GetCommonItem/'
                  }).then(function successCallback(response) {
                      //$scope.CommonFeilds.ClassInfo = filterFilter(response.data.results.ClassInfo, { VersionId: $scope.studentSearch.Version1.VersionId });
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.message);
                  });
              }
              $scope.GetGroup = function GetGroup(classid) {
                  $scope.Groups = new Array();
                  if (classid.length != 1)
                      return;
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Common/GetGroup/' + classid[0]
                  }).then(function successCallback(response) {
                      $scope.Groups = response.data.results;
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.message);
                  });
              }
              $scope.GetSection = function GetSection() {
                  $scope.Sections = [];
                  if ($scope.studentSearch.Class1.length != 1)
                      return;
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Common/GetSection/' + $scope.studentSearch.Session1[0] + "/" + $scope.studentSearch.Shift1[0] + "/" + $scope.studentSearch.Class1[0] 
                  }).then(function successCallback(response) {
                      $scope.Sections = response.data.results;
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.Message);
                  });
              }

              var log = [];
              $scope.StudentTypeArr = [{ 'Value': 'I', 'Text': 'Inactive' }, { 'Value': 'A', 'Text': 'Active' }, { 'Value': 'D', 'Text': 'Delete' }, { 'Value': 'WTC', 'Text': 'Inactive With TC' }]
              function getDistricts() {
                  $scope.Districts = [];
                  $http({
                      method: 'GET',
                      url: baseUrl + 'Common/GetDistrict/'
                  }).then(function successCallback(response) {
                      $scope.Districts = response.data.results;
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.message);
                  });
              }
              $scope.getPStations1 = function getPStations1() {
                  $scope.PStations1 = [];
                  if ($scope.studentSearch.PresentDistrictId1.length > 1)
                      return;
                  $http.get(baseUrl + 'Common/GetPoliceStaionByDId/' + $scope.studentSearch.PresentDistrictId1).then(function (response) {
                      $scope.PStations1 = response.data.results;
                  });
              }
              $scope.getPStations2 = function getPStations2() {
                  $scope.PStations2 = [];
                  if ($scope.studentSearch.PermanentDistrictId1.length > 1)
                      return;
                  $http.get(baseUrl + 'Common/GetPoliceStaionByDId/' + $scope.studentSearch.PermanentDistrictId1).then(function (response) {
                      $scope.PStations2 = response.data.results;
                  });
              }

              function Search() {
                  $scope.studentSearch.Routes1 = []
                  $scope.studentSearch.Statuses = createStringByArray($scope.studentSearch.Statuses1);
                  $scope.studentSearch.Shift = createStringByArray($scope.studentSearch.Shift1);
                  $scope.studentSearch.Session = createStringByArray($scope.studentSearch.Session1);
                  $scope.studentSearch.Section = createStringByArray($scope.studentSearch.Section1);
                  $scope.studentSearch.Class = createStringByArray($scope.studentSearch.Class1);
                  $scope.studentSearch.StuType = createStringByArray($scope.studentSearch.StuType1);
                  $scope.studentSearch.Branch = createStringByArray($scope.studentSearch.Branch1);
                  $scope.studentSearch.House = createStringByArray($scope.studentSearch.House1);
                  $scope.studentSearch.Gender = createStringByArray($scope.studentSearch.Gender1);
                  $scope.studentSearch.Religion = createStringByArray($scope.studentSearch.Religion1);
                  $scope.studentSearch.BloodGroup = createStringByArray($scope.studentSearch.BloodGroup1);
                  $scope.studentSearch.Quota = createStringByArray($scope.studentSearch.Quota1);
                  $scope.studentSearch.FatherEQ = createStringByArray($scope.studentSearch.FatherEQ1);
                  $scope.studentSearch.FatherProfession = createStringByArray($scope.studentSearch.FatherProfession1);
                  $scope.studentSearch.MotherEQ = createStringByArray($scope.studentSearch.MotherEQ1);
                  $scope.studentSearch.MotherProfession = createStringByArray($scope.studentSearch.MotherProfession1);
                  $scope.studentSearch.PresentDistrictId = createStringByArray($scope.studentSearch.PresentDistrictId1);
                  $scope.studentSearch.PresentThanaId = createStringByArray($scope.studentSearch.PresentThanaId1);
                  $scope.studentSearch.PermanentDistrictId = createStringByArray($scope.studentSearch.PermanentDistrictId1);
                  $scope.studentSearch.PermanentThanaId = createStringByArray($scope.studentSearch.PermanentThanaId1);
                  $scope.studentSearch.Interest = createStringByArray($scope.studentSearch.Interest1);
                  $scope.studentSearch.Organization = createStringByArray($scope.studentSearch.Organization1);
                  $scope.studentSearch.MemberofClub = createStringByArray($scope.studentSearch.MemberofClub1);
                  $scope.studentSearch.Routes = createStringByArray($scope.studentSearch.Routes1);
                  $scope.studentSearchs = [];
                  $http({
                      method: 'POST',
                      url: baseUrl + 'Student/StuDetailInfo/',
                      data: $scope.studentSearch
                  }).then(function successCallback(response) {
                      $scope.studentSearchs = response.data.results;
                      $scope.selectedAll = true;
                      angular.forEach($scope.studentSearchs, function (item) {
                          item.Selected = $scope.selectedAll;
                      });
                      Recipiant();
                      toastr["success"](response.data.message);
                  }, function errorCallback(response) {
                      toastr["warning"](response.data.message);
                  });
              };
              $scope.pageChanged = function () {
                  Search()
              };
              //This method is calling from dropDown
              $scope.changePageSize = function () {
                  $scope.studentSearch.pageIndex = 1;
                  Search();
              };
              function DropDownValue(a, b) {
                  var data = "";
                  $scope.array = b;
                  angular.forEach($scope.array, function (value, key) {
                      if (!isNaN(parseInt(value))) {
                          data = data.concat(value[a]).concat(',')
                      }
                      else {
                          data = $scope.array.toString().concat(',')
                      }
                  });
                  return "'" + data + "'";
              }
              function createStringByArray(array) {
                  var output = '';
                  angular.forEach(array, function (value, key) {
                      if (!isNaN(parseInt(value))) {
                          output += value + ',';
                      }
                      else {
                          output += "'" + value.toString() + '\',';
                      }
                  });
                  return output;
              }

              function datatimes() {
                  $scope.Dateformat = 'dd/MM/yyyy';
                  $scope.$watch('studentSearch.startDate', function (niu) {
                      if (niu) {
                          $scope.dateOptionsDOB.minDate = niu
                      }
                  });
                  $scope.$watch('studentSearch.AdstartDate', function (niu) {
                      if (niu) {
                          $scope.dateOptionsAd.minDate = niu
                      }
                  });
              }
          }])
    </script>
